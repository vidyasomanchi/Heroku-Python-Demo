    "from flask import *\n",
    "import pandas as pd\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route(\"/tables\")\n",
    "def show_tables():\n",
    "    data = pd.read_csv(r'Twitter_Analyized.csv')\n",
    "    return render_template('view.html',tables=[data.to_html(classes='report')])\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    app.run()"
   ]
